{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run dev server",
      "type": "python",
      "request": "launch",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}/src",
      "module": "uvicorn",
      "args": ["--port=8000", "--reload", "--ws=none", "--limit-concurrency=5", "main:app"]
    }
  ]
}
